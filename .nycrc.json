{
    "all": true,
    "include": ["src/**/*.ts"],
    "exclude": ["src/**/*.d.ts", "src/webview/**/*.ts", "test/**/*.ts"],
    "extension": [".ts"],
    "reporter": ["text", "text-summary", "html", "lcov"],
    "report-dir": "coverage",
    "temp-dir": ".nyc_output",
    "check-coverage": true,
    "branches": 60,
    "lines": 60,
    "functions": 60,
    "statements": 60,
    "per-file": false,
    "watermarks": {
        "lines": [60, 80],
        "functions": [60, 80],
        "branches": [60, 80],
        "statements": [60, 80]
    }
}
