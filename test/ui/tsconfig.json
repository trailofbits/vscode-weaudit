{
    "compilerOptions": {
        "module": "Node16",
        "target": "ES2022",
        "lib": ["ES2022"],
        "outDir": "../../out/test/ui",
        "rootDir": ".",
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "moduleResolution": "Node16"
    },
    "include": ["**/*.ts", "**/*.d.ts"],
    "exclude": ["node_modules"]
}
